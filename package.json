{
  "name": "electron-training-app",
  "version": "0.1.0",
  "description": "App created during electron training",
  "main": "main.js",
  "author": "Vitalii Pertsovych",
  "license": "MIT",
  "scripts": {
    "start:electron": "electron .",
    "watch:electron": "nodemon --exec electron .",
    "compile:sass": "node-sass sass/main.scss app/styles.css -w",
    "start": "npm-run-all --parallel compile:sass watch:electron"
  },
  "dependencies": {
    "electron": "14.2.1",
    "electron-window-state": "^5.0.3",
    "node-sass": "^7.0.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5"
  }
}
